{
  "name": "fastify-errors-properties",
  "version": "0.7.0",
  "description": "Fastify plugin to support http-errors additional properties.",
  "homepage": "https://sw.cowtech.it/fastify-errors-properties",
  "repository": "github:ShogunPanda/fastify-errors-properties",
  "keywords": [
    "fastify",
    "fastify-plugin",
    "http-errors"
  ],
  "bugs": {
    "url": "https://github.com/ShogunPanda/fastify-errors-properties/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "files": [
    "lib",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/*.ts test/*.ts",
    "test": "tap --reporter=spec --coverage-report=html --coverage-report=text --no-browser test/*.test.ts",
    "ci": "yarn lint && tap --no-color --reporter=spec --coverage-report=json --coverage-report=text --branches 90 --functions 90 --lines 90 --statements 90 test/*.test.ts",
    "prebuild": "rm -rf lib/* types/* && yarn lint",
    "build": "tsc -p .",
    "prepublishOnly": "yarn ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "ajv": "^6.12.0",
    "fastify-plugin": "^1.6.1",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "statuses": "^1.5.0"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^6.8.4",
    "@types/http-errors": "^1.6.3",
    "@types/node": "^13.9.0",
    "@types/statuses": "^1.5.0",
    "fastify": "^2.12.1",
    "prettier": "^1.19.1",
    "tap": "^14.10.6",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
